{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "purchase_products",
    "description": "The purchase Schrema",
    "required": [
        "purchase_id",
        "product_id",
        "count"
    ],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "id",
            "description": "the purchase id"
        },
        "count": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "count",
            "description": "the purchase product count"
        },
        "purchase_id": {
            "$id": "#/properties/purchase_date",
            "type": "integer",
            "title": "purchase_date",
            "description": "The purchase purchase_date."
        },
        "product_id": {
            "$id": "#/properties/product_id",
            "type": "number",
            "title": "product_id",
            "description": "The purchase product_id."
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "created_at",
            "description": "the creation date of purchase",
            "examples": [
                "2020-06-02T14:04:36.000Z"
            ]
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "updated_at",
            "description": "the purchase update date",
            "examples": [
                "2020-06-02T14:04:36.000Z"
            ]
        },
        "deleted": {
            "$id": "#/properties/deleted",
            "type": "string",
            "title": "deleted",
            "description": "The deleted purchase date",
            "examples": [
                "2020-06-02T14:04:36.000Z"
            ]
        }
    }
}