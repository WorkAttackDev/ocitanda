{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"title": "Category",
	"description": "The category Schrema",
	"required": ["name"],
	"additionalProperties": false,
	"properties": {
		"id": {
			"$id": "#/properties/id",
			"type": "integer",
			"title": "id",
			"description": "the category id"
		},
		"name": {
			"$id": "#/properties/name",
			"type": "string",
			"title": "name",
			"description": "the category name",
			"examples": ["Denilson Costa"]
		},
		"created_at": {
			"$id": "#/properties/created_at",
			"type": "string",
			"title": "created_at",
			"description": "the creation date of category",
			"examples": ["2020-06-02T14:04:36.000Z"]
		},
		"updated_at": {
			"$id": "#/properties/updated_at",
			"type": "string",
			"title": "updated_at",
			"description": "the category update date",
			"examples": ["2020-06-02T14:04:36.000Z"]
		},
		"deleted_at": {
			"$id": "#/properties/deleted_at",
			"type": "string",
			"title": "deleted_at",
			"description": "The deleted category date",
			"examples": ["2020-06-02T14:04:36.000Z"]
		}
	}
}
