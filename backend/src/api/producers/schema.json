{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"title": "Consumer",
	"description": "The Consumer Schrema",
	"required": [
		"name"
	],
	"additionalProperties": false,
	"properties": {
		"name": {
			"$id": "#/properties/name",
			"type": "string",
			"title": "name",
			"description": "the name of the farm",
			"examples": [
				"Ocitanda"
			]
		},
		"created_at": {
			"$id": "#/properties/created_at",
			"type": "string",
			"title": "created_at",
			"description": "the creation date of user",
			"examples": [
				"2020-06-02T14:04:36.000Z"
			]
		},
		"updated_at": {
			"$id": "#/properties/updated_at",
			"type": "string",
			"title": "updated_at",
			"description": "the user update date",
			"examples": [
				"2020-06-02T14:04:36.000Z"
			]
		},
		"deleted": {
			"$id": "#/properties/deleted",
			"type": "string",
			"title": "deleted",
			"description": "The deleted user date",
			"examples": [
				"2020-06-02T14:04:36.000Z"
			]
		}
	}
}