{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "User",
    "description": "The user Schrema",
    "required": [
        "product_id",
        "consumer_id"
    ],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "id",
            "description": "the user id"
        },
        "product_id": {
            "$id": "#/properties/product_id",
            "type": "integer",
            "title": "product_id",
            "description": "The product id"
        },
        "consumer_id": {
            "$id": "#/properties/consumer_id",
            "type": "integer",
            "title": "consumer_id",
            "description": "the consumer_id",
            "examples": [
                1, 2
            ]
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "created_at",
            "description": "the creation date of user",
            "examples": [
                "2020-06-02T14:04:36.000Z"
            ]
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "updated_at",
            "description": "the user update date",
            "examples": [
                "2020-06-02T14:04:36.000Z"
            ]
        },
        "deleted_at": {
            "$id": "#/properties/deleted_at",
            "type": "string",
            "title": "deleted_at",
            "description": "The deleted user date",
            "examples": [
                "2020-06-02T14:04:36.000Z"
            ]
        }
    }
}