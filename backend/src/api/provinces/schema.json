{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"title": "Province",
	"description": "The province Schrema",
	"required": ["name"],
	"additionalProperties": false,
	"properties": {
		"id": {
			"$id": "#/properties/id",
			"type": "integer",
			"title": "id",
			"description": "the province id"
		},
		"name": {
			"$id": "#/properties/name",
			"type": "string",
			"title": "name",
			"description": "the province name",
			"examples": ["Denilson Costa"]
		},
		"created_at": {
			"$id": "#/properties/created_at",
			"type": "string",
			"title": "created_at",
			"description": "the creation date of province",
			"examples": ["2020-06-02T14:04:36.000Z"]
		},
		"updated_at": {
			"$id": "#/properties/updated_at",
			"type": "string",
			"title": "updated_at",
			"description": "the province update date",
			"examples": ["2020-06-02T14:04:36.000Z"]
		},
		"deleted": {
			"$id": "#/properties/deleted",
			"type": "string",
			"title": "deleted",
			"description": "The deleted province date",
			"examples": ["2020-06-02T14:04:36.000Z"]
		}
	}
}
