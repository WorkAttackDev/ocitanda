{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "User",
    "description": "The user Schrema",
    "required": [
        "price",
        "name",
        "description",
        "image_url",
        "quantity"
    ],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "id",
            "description": "the product id"
        },
        "price": {
            "$id": "#/properties/price",
            "type": "number",
            "title": "price",
            "description": "The product price"
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "name",
            "description": "the product name",
            "examples": [
                "Arroz"
            ]
        },
        "quantity": {
            "$id": "#/properties/quantity",
            "type": "integer",
            "description": "the product quantity",
            "examples": [
                100,
                1000
            ]
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "description",
            "description": "last login",
            "examples": [
                "Lorem ipsum dolor"
            ]
        },
        "producer_id": {
            "$id": "#/properties/producer_id",
            "type": "integer",
            "title": "producer id",
            "description": "producer id",
            "examples": [
                1, 2
            ]
        },
        "category_id": {
            "$id": "#/properties/category_id",
            "type": "integer",
            "title": "category id",
            "description": "category id",
            "examples": [
                1, 2
            ]
        },
        "image_url": {
            "$id": "#/properties/image_url",
            "type": "string",
            "title": "image_url",
            "description": "Aplication Image",
            "examples": [
                "https://wwfjdkfjksdjflk.com/fkdsfk"
            ]
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "created_at",
            "description": "the creation date of user",
            "examples": [
                "2020-06-02T14:04:36.000Z"
            ]
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "updated_at",
            "description": "the user update date",
            "examples": [
                "2020-06-02T14:04:36.000Z"
            ]
        },
        "deleted": {
            "$id": "#/properties/deleted",
            "type": "string",
            "title": "deleted",
            "description": "The deleted user date",
            "examples": [
                "2020-06-02T14:04:36.000Z"
            ]
        }
    }
}